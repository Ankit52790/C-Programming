#include<stdio.h>
 int main()
 { 
char c;
 int lwrcase_vowel, uprcase_vowel;
 printf("Enter an alphabet: ");
 scanf("%c", &c);
 lwrcase_vowel = (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u'); 
 uprcase_vowel = (c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U'); 
 if (lwrcase_vowel || uprcase_vowel) 
printf("%c is a vowel.", c);
 else
 printf("%c is a consonant.", c);
 return 0; 
}
