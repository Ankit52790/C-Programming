#include<stdio.h>

void swap(int int);
int main() 
{
 int a=30;
 int b=20;
    printf("before swapping the value in main a=%d,b=%d\n",a,b);
    swap(a,b); 
    printf("after swapping the value in main a=%d,b=%d\n", a, b
 }
  
void swap (int a, int b) 
{
  int temp;
temp=a;
b=temp;
printf("after swapping the the value in main a=%d,b=%d\n",a,b);
}
