#include<stdio.h>

int main()
{
 int a,b,c,d;
    printf("enter three numbers a,b and c");
    scanf("%d%d%d",&a,&b,&c);
    
    if((a>b)&&(a>c)) 
    {
    printf("%d is maximum",a);
    }
    if((b>a)&&(b>c)) 
    {
    printf("%d is maximum",b);
    }
    if((c>b)&&(c>a))
    {
    printf("%d is maximum",c);}
    return 0;
}
