#include<stdio.h>
#include<conio.h>
#define MAXSIZE 50
void bubble (int x[],int n) 
{
 int i,j,temp;
for(i=0;i<n;i++)
{
if(j=0;j<n-1;j++)
{
 if(x[j]>x[j+1])
{
 temp=x[j];
x[j]=x[j+1];
x[j+1]=temp;
}
}
}
}
 void main() 
{
int x[MAXISIZE],n,i;
printf("Enter the number of elements:");
scanf("%d",&n);
printf("\nEnter the elements:\n");
for(i=0;i<n;i++)
scanf("%d",&x[i]);
bubble(x,n);
printf("\nThe sorted output:\n");
for(i=0;i<n;i++)
printf("\n%d",x[i]);
getch();
}
